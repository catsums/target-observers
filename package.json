{
    "name": "@catsums/targetobservers",
    "version": "1.1.2",
    "description": "Observers that can be used for checking specific changes in objects. This mainly consists of a Transform Observer and a Collision Observer for DOM Elements",
    "types": "./lib/index.d.ts",
    "main": "./lib/browser/index.js",
    "module": "./lib/index.js",
    "exports": {
        ".": {
            "require": "./lib/cjs/index.js",
            "import": "./lib/esm/index.js"
        }
    },
    "scripts": {
        "bundle": "node --loader ts-node/esm build.mjs",
        "build": "npm run bundle",
        "update": "node update.mjs",
        "npm-publish": "npm publish --access public"
    },
    "keywords": [
        "observer",
        "collisions",
        "collision-detection",
        "position",
        "position-observer",
        "collision-observer",
        "clip-path-collision"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.20.2",
        "babel-loader": "^9.1.2",
        "esbuild": "^0.19.5",
        "esbuild-plugin-umd-wrapper": "^2.0.0",
        "esmify": "^2.1.1",
        "npm-dts": "^1.3.12",
        "ts-node": "^10.9.1"
    },
    "dependencies": {
        "@catsums/my": "^1.2.1",
        "@catsums/vector2": "^1.3.1",
        "lodash": "^4.17.21",
        "svg-path-parser": "^1.1.0"
    }
}